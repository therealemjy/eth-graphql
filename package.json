{
  "name": "eth-graphql-repository",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "test:e2e": "turbo run test:e2e",
    "test:units": "turbo run test:units",
    "test": "npm run test:units && npm run test:e2e",
    "lint": "turbo run lint",
    "tsc": "turbo run tsc",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,md,json,sol}\"",
    "publish-lib": "turbo run build lint && changeset version && changeset publish"
  },
  "devDependencies": {
    "@changesets/cli": "^2.26.1",
    "@typescript-eslint/eslint-plugin": "^5.59.1",
    "eslint": "^7.32.0",
    "eslint-config-custom": "*",
    "prettier": "^2.5.1",
    "prettier-plugin-solidity": "^1.1.3",
    "turbo": "latest"
  },
  "workspaces": [
    "example",
    "packages/*"
  ]
}
