{
  "$schema": "https://turbo.build/schema.json",
  "globalDependencies": [".env"],
  "pipeline": {
    "build": {
      "outputs": [".next/**", "!.next/cache/**", "dist/**"],
      "dependsOn": ["^build"]
    },
    "lint": {},
    "tsc": {},
    "dev": {
      "cache": false,
      "persistent": true
    },
    "test:units": {
      "cache": false
    },
    "test:e2e": {
      "cache": false,
      "dependsOn": ["^build"]
    }
  }
}
